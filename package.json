    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.4.1",
    "globals": "15.14.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3"
  },
  "lint-staged": {
    "**/*.js": [
